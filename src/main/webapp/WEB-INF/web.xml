<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>


  <servlet>
    <servlet-name>ParserResult</servlet-name>
    <display-name>ParserResult</display-name>
    <jsp-file>/WEB-INF/jsp/ParserResult.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>ParserResult</servlet-name>
    <url-pattern>/ParserResult</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>FrontController</servlet-name>
    <display-name>FrontController</display-name>
    <servlet-class>by.tr.web.controller.FrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FrontController</servlet-name>
    <url-pattern>/FrontController</url-pattern>
  </servlet-mapping>

</web-app>
